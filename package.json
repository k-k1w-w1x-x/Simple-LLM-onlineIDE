{
  "name": "cscupcakecoder",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "start": "pnpm run build && pnpm run electron",
    "electron-pack": "electron-builder"
  },
  "keywords": [
    "Monaco Editor",
    "Vue-Web-Terminal",
    "Vue-Web-IDE",
    "Web Container",
    "在线代码编辑器",
    "Online Code Editor"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/node": "^20.13.0",
    "@webcontainer/api": "^1.1.9",
    "ansi-to-html": "^0.7.2",
    "babel-runtime": "^6.26.0",
    "element-plus": "^2.7.4",
    "express": "^4.21.1",
    "less": "^4.2.0",
    "less-loader": "4",
    "markdown-it": "^14.1.0",
    "monaco-editor": "^0.49.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-markdown": "^2.2.4",
    "vue-web-terminal": "3"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "9.x",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.3.0",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.11.0",
    "vite": "^5.4.9",
    "vue-tsc": "^2.0.6"
